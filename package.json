   "scripts": {
      "build:client": "cross-env NODE_ENV=react TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config webpack/webpack.production.client.ts",
      "serve:client": "cross-env NODE_ENV=react TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack-dev-server --config webpack/webpack.development.client.ts",
      "build:server": "cross-env NODE_ENV=koa   TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config webpack/webpack.common.server.ts",
      "serve:server": "nodemon --watch src/server --ext .js,.ts --exec \"npm run build:server -- -d && node dist/server\"",
      "serve": "npm-run-all --parallel serve:*",
      "build": "npm run build:client && npm run build:server -- --mode production",
      "start": "node dist/server",
      "lint": "eslint src/**/*.*",
      "script:gql-gen": "graphql-codegen --config codegen.yml",
      "script:generateIcon": "node scripts/generateIcon.js",
      "script:generateGoogleActionsTypes": "ts-node scripts/generateGoogleActionsTypes.ts src/common/GoogleActionsTypes.ts"
   },
